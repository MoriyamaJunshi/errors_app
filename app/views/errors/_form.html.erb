<%= form_with(model: error) do |f| %>
  <% if error.errors.any? %>
    <div id="error_explanation">
      <ul>
        <% error.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <%= f.label :content, 'エラータイトル' %>
  <%= f.text_field :content %>

  <%= f.submit '投稿' %>
<% end %>